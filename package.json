{
  "name": "schematyc-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:prod": "env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
    "format": "prettier --write src/**/*.{js,ts,tsx,css} --config ./.prettierrc",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@szhsin/react-menu": "^3.5.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-modal": "^3.16.0",
    "ag-grid-community": "^29.3.4",
    "ag-grid-enterprise": "^29.3.4",
    "ag-grid-react": "^29.3.4",
    "axios": "^1.4.0",
    "debounce": "^1.2.1",
    "env-cmd": "^10.1.0",
    "file-saver": "^2.0.5",
    "lodash-es": "^4.17.21",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-datepicker": "4.16.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-pro-sidebar": "^1.1.0-alpha.1",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1",
    "react-select": "^5.7.2",
    "react-sliding-side-panel": "^2.0.3",
    "react-switch": "^7.0.0",
    "react-tabs": "^6.0.2",
    "react-to-print": "^2.14.13",
    "react-toastify": "^9.1.3",
    "react-tooltip": "^5.21.4",
    "rxjs": "^7.8.0",
    "styled-components": "^5.3.9",
    "styled-system": "^5.1.5",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/preset-create-react-app": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@storybook/react-webpack5": "^7.0.6",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/debounce": "^1.2.1",
    "@types/file-saver": "^2.0.5",
    "@types/lodash-es": "^4.17.7",
    "@types/object-assign": "^4.0.30",
    "@types/react-datepicker": "^4.15.0",
    "@types/styled-components": "^5.1.26",
    "@types/styled-system": "^5.1.16",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.11",
    "prettier": "2.8.7",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.6",
    "webpack": "^5.79.0"
  }
}
